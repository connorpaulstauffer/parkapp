<% provide(:title, 'All parks') %>
  
<div class="container-fluid"> 
  <div class="row">
    <div class="col-md-4">   
      <h3 id="parks-title">All parks</h3>
      <ul class="parks"> 
        <%= render @parks %>
      </ul>
      
      <%= will_paginate %>
    </div>
    <div class="col-md-8">
      <% if Image.any? %>
        <h3>Images (<%= Image.count %>)</h3>
        <ol class="reviews row image-stream">
          <%= render @images %>
        </ol>
        <%= will_paginate @images %>
      <% end %>
    </div>
  </div>
</div>